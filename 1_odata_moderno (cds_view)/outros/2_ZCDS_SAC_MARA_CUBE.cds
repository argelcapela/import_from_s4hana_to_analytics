@AbapCatalog.sqlViewName: 'ZCDSSMACU' // Nome da visão SQL no banco de dados
@AbapCatalog.compiler.compareFilter: true // Ativa a comparação de filtros durante a compilação
@AbapCatalog.preserveKey: true // Garante que as chaves (campos principais) sejam preservadas
@AccessControl.authorizationCheck: #NOT_REQUIRED // Não precisa de checagem de autorização
@EndUserText.label: 'ZCDS_SAC_MARA_CUBE' // Nome que aparece para o usuário final

@ObjectModel: { usageType: { sizeCategory: #XXL, // Define que o volume de dados é muito grande (XXL)
                dataClass:  #MIXED, // Indica que os dados são mistos (diferentes tipos de informações)
                serviceQuality: #D }, // Qualidade de serviço indicada como D
                supportedCapabilities: [#ANALYTICAL_PROVIDER, #SQL_DATA_SOURCE, #CDS_MODELING_DATA_SOURCE], // Funcionalidades que a visão suporta
                modelingPattern: #ANALYTICAL_CUBE } // Define que esta visão é um "Cubo Analítico"
@Metadata: { ignorePropagatedAnnotations: true, // Ignora anotações propagadas (herdadas de outras visões)
             allowExtensions: true } // Permite que outras visões estendam (ou usem) essa visão
@VDM: { lifecycle.contract.type: #PUBLIC_LOCAL_API, // Indica que a visão é pública para uso local
        viewType: #COMPOSITE } // Tipo da visão é composta, ou seja, junta dados de várias tabelas
@Analytics.dataCategory: #CUBE // Categoria de dados é "Cubo Analítico" (bom para relatórios e análises)

define view ZCDS_SAC_MARA_CUBE as select from mara // Seleciona os dados da tabela MARA (dados principais de materiais)
left outer join t023t // Faz uma junção externa à esquerda com a tabela T023T (descrição de grupos de mercadorias)
    on  mara.matkl = t023t.matkl // Junta as tabelas pelo campo MATKL (grupo de mercadorias)
    and t023t.spras = 'P' // Junta apenas onde o idioma é português (spras = 'P')
left outer join makt // Faz uma junção externa à esquerda com a tabela MAKT (descrição de materiais)
    on  mara.matnr = makt.matnr // Junta as tabelas pelo campo MATNR (número do material)
    and makt.spras = 'P' // Junta apenas onde o idioma é português (spras = 'P')
{

    key mara.matnr,
